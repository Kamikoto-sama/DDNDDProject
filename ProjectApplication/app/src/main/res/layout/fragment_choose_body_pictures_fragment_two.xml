<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:context=".ChooseBodyPicturesFragmentTwo"
        android:background="@color/green">

    <ImageView
            android:id="@+id/choose_body_picture5"
            android:src="@drawable/choose_body_picture5"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintWidth_default="percent"
            app:layout_constraintHeight_default="percent"
            app:layout_constraintWidth_percent="0.48"
            app:layout_constraintHeight_percent="0.48"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/choose_body_picture6"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/choose_body_picture7"/>

    <ImageView
            android:id="@+id/choose_body_picture6"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintWidth_default="percent"
            app:layout_constraintHeight_default="percent"
            app:layout_constraintWidth_percent="0.48"
            app:layout_constraintHeight_percent="0.48"
            android:src="@drawable/choose_body_picture6"
            app:layout_constraintLeft_toRightOf="@id/choose_body_picture5"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/choose_body_picture8"
            app:layout_constraintRight_toRightOf="parent"/>

    <ImageView
            android:id="@+id/choose_body_picture7"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintWidth_default="percent"
            app:layout_constraintHeight_default="percent"
            app:layout_constraintWidth_percent="0.48"
            app:layout_constraintHeight_percent="0.48"
            android:src="@drawable/choose_body_picture7"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/choose_body_picture8"
            app:layout_constraintTop_toBottomOf="@id/choose_body_picture5"
            app:layout_constraintBottom_toBottomOf="parent"/>

    <ImageView
            android:id="@+id/choose_body_picture8"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintWidth_default="percent"
            app:layout_constraintHeight_default="percent"
            app:layout_constraintWidth_percent="0.48"
            app:layout_constraintHeight_percent="0.48"
            android:src="@drawable/choose_body_picture8"
            app:layout_constraintLeft_toRightOf="@id/choose_body_picture7"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/choose_body_picture6"
            app:layout_constraintBottom_toBottomOf="parent"/>

</android.support.constraint.ConstraintLayout>